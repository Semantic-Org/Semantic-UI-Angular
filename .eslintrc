{
  "rules" : {
    "indent": [2, 2],
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-invalid-regexp": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "max-len": [2, 120, 4],
    "strict": [2, "function"],
    "no-trailing-spaces": 2,
    "no-with": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 2,
    "space-return-throw-case": 2,
    "space-before-blocks": 2,
    "space-unary-ops": 2,
    "space-after-keywords": 2,
    "space-infix-ops": 2,
    "no-spaced-func": 2,
    "space-before-function-paren": [2, "never"],
    "block-spacing": [2, "always"],
    "comma-dangle": [2, "never"],
    "linebreak-style": 2,
    "eol-last": 2
  },
  "globals": {
    "angular": false,
    "describe": false,
    "it": false,
    "beforeEach": false,
    "afterEach": false,
    "inject": false,
    "module": false,
    "jasmine": false,
    "browser": false,
    "expect": false,
    "_": false,
    "$": false
  },
  "env" : {
    "browser": true
  }
}
